/* components/loading/cmp.wxss */
.container{
    position: relative;
    width: 60rpx;
    height: 60rpx;
    margin: 0 auto;
}
.circle,
.circle1{
    position: absolute;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    background-color: rgba(0,0,0,0.4);
    animation: change 2s infinite;
    border-radius: 50%;
}
.circle{
    animation-delay: -1s;
}
@keyframes change{
    0%{
        transform: scale(0)
    }
    50%{
        transform: scale(1)
    }
    100%{
        transform: scale(0)
    }
}